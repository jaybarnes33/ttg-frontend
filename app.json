{
  "expo": {
    "name": "ttg",
    "slug": "ttg",
    "version": "1.0.0",
    "scheme": "ttg",
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-build-properties",
      [
        "expo-notifications",
        {
          "enableBackgroundRemoteNotifications": true,
          "sounds": ["./assets/sounds/ring.mp3"]
        }
      ],
      [
        "react-native-iap",
        {
          "paymentProvider": "both"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.dishplug.ttg",
      "infoPlist": {
        "UIBackgroundModes": ["remote-notification"],
        "NSUserActivityTypes": ["INStartCallIntent", "INStartVideoCallIntent"],
        "SKAdNetworkItems": [
          {
            "SKAdNetworkIdentifier": "cstr6suwn9.skadnetwork"
          }
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.dishplug.ttg",
      "googleServicesFile": "./google-services.json",
      "permissions": ["NOTIFICATIONS", "BILLING"]
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "9e45f1a8-1481-4948-aca1-3613c99625ee"
      }
    },
    "owner": "dishplug"
  }
}
